{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mf-angular-app": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "mfapp",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "../../dist/mf-angular-app-esm",
            "index": "src/index.html",
            "main": "src/main.single-spa.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/@avla/ui-design/assets",
                "output": "assets/"
              }
            ],
            "styles": ["src/styles.css"],
            "stylePreprocessorOptions": {
              "includePaths": ["src"]
            },
            "scripts": [],
            "customWebpackConfig": {
              "path": "extra-webpack.config.js",
              "libraryName": "mf-angular-app",
              "libraryTarget": "module"
            },
            "deployUrl": "http://localhost:4200/"
          },
          "configurations": {
            "production": {
              "budgets": [
                { "type": "initial", "maximumWarning": "500kB", "maximumError": "1MB" },
                { "type": "anyComponentStyle", "maximumWarning": "2kB", "maximumError": "4kB" }
              ],
              "outputHashing": "none",
              "deployUrl": "http://localhost:4200/"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "outputHashing": "none",
              "deployUrl": "http://localhost:4200/"
            },
            "single-spa-prod": {
              "main": "src/main.single-spa.ts",
              "index": "src/index.html",
              "outputPath": "../../dist/mf-angular-app-esm",
              "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "glob": "**/*",
                  "input": "node_modules/@avla/ui-design/assets",
                  "output": "assets/"
                }
              ],
              "scripts": [],
              "styles": ["src/styles.css"],
              "stylePreprocessorOptions": { "includePaths": ["src"] },
              "vendorChunk": false,
              "optimization": true,
              "sourceMap": false,
              "buildOptimizer": true,
              "outputHashing": "none",
              "deployUrl": "http://localhost:4200/"
            },
            "single-spa": {
              "main": "src/main.single-spa.ts",
              "index": "src/index.html",
              "outputPath": "../../dist/mf-angular-app-esm",
              "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "glob": "**/*",
                  "input": "node_modules/@avla/ui-design/assets",
                  "output": "assets/"
                }
              ],
              "scripts": [],
              "styles": ["src/styles.css"],
              "stylePreprocessorOptions": { "includePaths": ["src"] },
              "vendorChunk": false,
              "optimization": false,
              "sourceMap": true,
              "buildOptimizer": false,
              "outputHashing": "none",
              "deployUrl": "http://localhost:4200/"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": { "buildTarget": "mf-angular-app:build:production" },
            "development": { "buildTarget": "mf-angular-app:build:development" },
            "single-spa": { "buildTarget": "mf-angular-app:build:single-spa" }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": { "builder": "@angular-devkit/build-angular:extract-i18n" },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "stylePreprocessorOptions": { "includePaths": ["src"] },
            "scripts": []
          }
        }
      }
    }
  },
  "cli": { "analytics": "e977f831-9b05-4b45-ac78-3b75de73d33b" }
}